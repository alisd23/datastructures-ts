{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["ES2023"],
    "module": "commonjs",
    "rootDir": "./",
    /* Emit */
    "declaration": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    /* Type Checking */
    "strict": true,
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "skipLibCheck": true
  }
}
